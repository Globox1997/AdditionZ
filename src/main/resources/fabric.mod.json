{
  "schemaVersion": 1,
  "id": "additionz",
  "version": "${version}",
  "name": "AdditionZ",
  "description": "Adds a couple of additions",
  "authors": [
    "Globox_Z"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/additionz",
    "sources": "https://github.com/Globox1997/AdditionZ",
    "issues": "https://github.com/Globox1997/AdditionZ/issues"
  },
  "license": "MIT",
  "icon": "assets/additionz/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.additionz.AdditionMain"
    ],
    "client": [
      "net.additionz.AdditionClient"
    ],
    "modmenu": [
      "net.additionz.config.ModMenuIntegration"
    ],
    "jade": [
      "net.additionz.waila.AgeJadePlugin"
    ],
    "emi": [
      "net.additionz.compat.AdditionzEmiPlugin"
    ],
    "rei_client": [
      "net.additionz.compat.AdditionzReiPlugin"
    ]
  },
  "mixins": [
    "additionz.mixins.json"
  ],
  "accessWidener" : "additionz.accesswidener",
  "depends": {
    "java": ">=17",
    "minecraft": ">=${minecraft_version}",
    "fabric-api": ">=${fabric_version}",
    "fabricloader": ">=${loader_version}",
    "cloth-config2": ">=${cloth_config_version}"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "mc-publish": {
      "modrinth": "QeSwHM7w",
      "curseforge": 659920
    },
    "waila:plugins": {
      "id": "additionz:plugin",
      "initializer": "net.additionz.waila.AgeWailaPlugin"
    }
  }
}